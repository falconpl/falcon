import Syn.*

> "Homoiconic While with single child"
> "Composing a = 0; while a < 5; > \"A: \", ++a; end"

a = 0
expr = While( LT($a,5), FastPrintNL( "A: ", PreInc($a) ) )
expr()

> "Done."
